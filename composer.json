{
    "name": "cbagdawala/laravel-id-generator",
    "description": "Easy way to generate custom ID in laravel framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Chintan Bagdawala",
            "email": "chintan@bagdawala.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.6.0"
    },
    "autoload": {
        "psr-4": {
            "cbagdawala\\LaravelIdGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "cbagdawala\\LaravelIdGenerator\\IdGeneratorServiceProvider"
            ]
        }
    }
}
